# Alias
unalias lsd
unalias ls
alias lsd='/usr/bin/lsd'


# lsd-control
[[ -f /usr/local/lib/lsd-alias ]] && source /usr/local/lib/lsd-alias

# aptpac
alias apt='aptpac'
alias apt-get='aptpac'

# no-beep less
alias less='less -Q'